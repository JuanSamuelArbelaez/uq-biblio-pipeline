@article{SAHRAOUI2025111054,
title = {Inflammatory myofibroblastic tumor of the adrenal gland: A case report},
journal = {International Journal of Surgery Case Reports},
volume = {128},
pages = {111054},
year = {2025},
issn = {2210-2612},
doi = {https://doi.org/10.1016/j.ijscr.2025.111054},
