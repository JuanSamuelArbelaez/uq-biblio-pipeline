@article{AHMED2025660,
title = {A Primer on Large Language Models (LLMs) and ChatGPT for Cardiovascular Healthcare Professionals},
journal = {CJC Open},
volume = {7},
number = {5},
pages = {660-666},
year = {2025},
issn = {2589-790X},
doi = {https://doi.org/10.1016/j.cjco.2025.02.012},
url = {https://www.sciencedirect.com/science/article/pii/S2589790X25001106},
author = {Muneeb Ahmed and Jeffrey Lam and Alexander Chow and Chi-Ming Chow},
abstract = {Generative artificial intelligence (AI), particularly large language models (LLMs), such as ChatGPT, is transforming healthcare by offering novel ways to synthesize and communicate medical knowledge. This development is especially relevant in cardiology, as patient education, clinical decision-making, and administrative workflows play pivotal roles in this area. ChatGPT, originally built on GPT-3 and refined into GPT-4, can simplify complex cardiology literature, translate technical explanations into plain language, and address questions across different linguistic backgrounds. Studies show that although ChatGPT demonstrates considerable promise in performing text-based tasks—ranging from passing portions of the European Exam in Core Cardiology to creating patient-friendly educational materials—its inability to interpret images remains a major limitation. Meanwhile, concerns around false information, data bias, and ethical issues highlight the need for careful oversight. Future directions include integrating LLMs with computer-vision modules for image-based diagnostics and combining unstructured patient data to improve risk prediction and phenotyping. Social-media research suggests that chatbots sometimes provide more-empathetic responses than do physicians, underscoring both their potential advantages and complexities. LLM-based tools can also generate letters for insurance prior authorizations or appeals, helping reduce administrative burden. New multimodal approaches, such as ChatGPT Vision, have the potential to enable direct image processing, although clinical validation of this function is yet to be established. The judicious integration of ChatGPT and other LLMs into cardiology requires ongoing validation, robust regulatory frameworks, and strong ethical guidelines to ensure patient privacy, avoid misinformation, and promote equitable healthcare delivery. This review aims to provide a primer on LLMs for cardiovascular professionals, summarizing key applications, current limitations, and prospects in this rapidly evolving field of digital health.
Résumé
L'intelligence artificielle (IA) générative, en particulier les grands modèles de langage (LLM) tels que ChatGPT, transforme les soins de santé en offrant de nouveaux moyens de synthétiser et de communiquer les connaissances médicales. Ceci est particulièrement pertinent en cardiologie, où l'éducation des patients, la prise de décision clinique et les flux de travail administratifs jouent un rôle central. ChatGPT, basé à l'origine sur GPT-3 et affiné dans GPT-4, peut simplifier la littérature complexe en cardiologie, traduire des explications techniques en langage clair et répondre à des questions provenant de différents contextes linguistiques. Les études montrent que si ChatGPT démontre un potentiel très prometteur pour les tâches basées sur le texte - qui vont de la réussite de certaines parties de l'examen européen de cardiologie de base à la création de matériel éducatif accessible aux patients - son incapacité à interpréter les images reste une limitation majeure. Par ailleurs, les inquiétudes concernant les renseignements faux, les biais de données et les questions éthiques soulignent la nécessité d'une supervision attentive. Les orientations futures comprennent l'intégration des LLM avec des modules de vision par ordinateur pour les diagnostics basés sur l'image et la combinaison de données non structurées sur les patients pour améliorer la prédiction des risques et le phénotypage. Les recherches sur les médias sociaux suggèrent que les agents conversationnels peuvent parfois fournir des réponses plus empathiques que les médecins, révélant des opportunités et les complications inhérentes. Pour les tâches administratives, les outils basés sur les LLM peuvent générer des lettres pour les demandes d'autorisation préalable ou des appels, contribuant ainsi à réduire la charge administrative. De nouvelles approches multimodales, telles que « ChatGPT Vision », pourraient à l'avenir permettre un traitement direct des images, bien que la validation clinique reste à établir. En fin de compte, l'intégration judicieuse de ChatGPT et d'autres LLM en cardiologie nécessitera une validation continue, des cadres réglementaires solides et des lignes directrices éthiques strictes pour garantir la confidentialité des patients, éviter la désinformation et promouvoir une prestation de soins de santé équitable. Cette étude a pour but de fournir aux professionnels de la santé cardiovasculaire une introduction aux LLM, en résumant les applications clés, les limites actuelles et les perspectives dans ce domaine de la santé numérique qui évolue rapidement.}
}
@article{DONNELLAN2024113804,
title = {Macrocephaly Following the Bidirectional Glenn Procedure},
