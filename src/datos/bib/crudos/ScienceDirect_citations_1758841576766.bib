@article{XUE2025131581,
title = {VQ-learning: Towards unbiased action value estimation in reinforcement learning},
journal = {Neurocomputing},
pages = {131581},
year = {2025},
issn = {0925-2312},
doi = {https://doi.org/10.1016/j.neucom.2025.131581},
url = {https://www.sciencedirect.com/science/article/pii/S0925231225022532},
author = {Yuan Xue and Daniel Kudenko and Megha Khosla},
keywords = {Reinforcement learning, Value overestimation, Sample efficiency},
abstract = {Q-learning, as a well-known Reinforcement Learning algorithm, is prone to overestimation of action values. Such an overestimation is mainly due to the use of the maximization operator when updating the Q function. Deep Q Networks (DQN) suffer from the same issue, which is further aggravated by the noisy learning environment, leading to degradation of performance. In this work, we propose a simple yet effective approach called VQ-learning, which addresses the overestimation issue, along with its deep learning extension, Deep VQ-Networks (DVQN). VQ-learning incorporates a state value function V and an action value function Q, both V and Q function are updated without using the maximization operator. While various methods have been developed to mitigate overestimation, we demonstrate that VQ-learning achieves superior performance. Even in situations where the maximization operator does not lead to overestimation bias, VQ-learning still remains the best sample efficiency, matching the performance of Expected SARSA while requiring lower computational cost. Furthermore, as the size of the action space increases, VQ-learning consistently maintains robust learning efficiency compared to baseline methods. We evaluate DVQN across ten Atari game domains and demonstrate that it delivers performance that is either superior to or comparable with Deep Q Networks, Deep SARSA, Deep Double Q Networks, Clipped Deep Double Q Networks, Averaged DQN, and Dueling DQN, both in terms of reward outcomes and sample efficiency.}
}
