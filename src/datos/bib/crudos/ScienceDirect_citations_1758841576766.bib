@article{NAJAFABADI2025,
title = {Hybrid AI in synthetic biology: next era in agriculture},
journal = {Trends in Plant Science},
year = {2025},
issn = {1360-1385},
doi = {https://doi.org/10.1016/j.tplants.2025.08.011},
